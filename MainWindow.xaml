<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tarea2Lab"
        xmlns:Accessibility="clr-namespace:Accessibility;assembly=Accessibility" x:Class="Tarea2Lab.MainWindow"
        mc:Ignorable="d"
        Title="Principal" Height="400" Width="400">
    <Grid>
        <StackPanel>
            <Menu Height="18">
                <MenuItem Header="Registros">
                    <MenuItem x:Name="RolesMenuItem" Header="Registro de Roles" Click="RolesMenuItem_Click"></MenuItem>
                    <MenuItem x:Name="UsuarioMenuItem" Header="Registro de Usuarios" Click="UsuarioMenuItem_Click"></MenuItem>

                </MenuItem>

                <MenuItem Header="Ayuda">
                    <Menu/>
                </MenuItem>
            </Menu>
        </StackPanel>
    </Grid>
</Window>
